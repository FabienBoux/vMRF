

Model.geo.res       = [64];
Model.geo.vasc.N    = [20];

%[p1,p2,p3,p4]       = ndgrid(0.01:0.24/29:0.25, [1:9/29:10]*1e-6, [40:200/19:240]*1e-3, [500:900/19:1400]*1e-12);
[p1,p2,p3]          = ndgrid(0.01:0.24/9:0.25, [1:9/9:10]*1e-6, [20:280/9:300]*1e-3);
[p1,p2,p3]          = ndgrid(0.1, 5*1e-6, [20:280/99:300]*1e-3);

Model.geo.vasc.Vf   = repmat(reshape(p1,1,[]), 1,2);
Model.geo.vasc.R    = repmat(reshape(p2,1,[]), 1,2);
Model.phy.vasc.T2   = repmat(reshape(p3,1,[]), 1,2);

Model.phy.B0        = [9.4];
